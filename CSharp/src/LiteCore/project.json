{
  "version": "1.0.0-*",
  "dependencies": {
    "NETStandard.Library": "1.6.0"
  },
  "frameworks": {
    "netstandard1.1": {
      "imports": "dnxcore50"
    }
  },
  "buildOptions": {
    "allowUnsafe": true,
    "outputName": "LiteCore-Interop",
    "debugType": "portable",
    "copyToOutput": {
       "include": ["libLiteCore.dylib", "LiteCore.dll"],
       "mappings": {
         "libLiteCore.dylib": "../../native/libLiteCore.dylib", 
         "LiteCore.dll": "../../native/LiteCore.dll"
       }
    }
  }
}